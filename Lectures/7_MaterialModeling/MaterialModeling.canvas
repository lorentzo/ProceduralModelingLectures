		{"id":"441bf7cdf45ebe6d","x":3680,"y":-220,"width":2960,"height":1243,"type":"group","label":"Practical examples"},
		{"id":"d3e04dbfd82a85b6","x":7020,"y":-700,"width":1807,"height":1942,"type":"group","label":"Further learning"},
